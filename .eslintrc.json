{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/naming-convention": [
   "error",
   {
      "selector": "default",
      "format": ["camelCase"]
   },
   {
      "selector": "variable",
      "format": ["camelCase", "UPPER_CASE"],
      "leadingUnderscore": "allow"
   },
   {
      "selector": "typeLike",
      "format": ["PascalCase"],
      "leadingUnderscore": "allow"
   },
   {
      "selector": "enumMember",
      "format": ["camelCase", "UPPER_CASE"]
   }
]
  }
}
